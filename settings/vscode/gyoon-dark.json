{
	"editor.fontSize": 14,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"editor.fontFamily": "JetBrainsMono Nerd Font",
	"C_Cpp.default.compilerPath": "/usr/bin/gcc",
	"update.showReleaseNotes": false,

	"terminal.integrated.fontSize": 14,
	"terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",

	// Zen Mode
	"zenMode.centerLayout": false,
	"zenMode.fullScreen": false,

	// Git
  	"git.confirmSync": false,
  	"git.autofetch": true,

	// 42 header
	"42header.email": "gyoon@student.42seoul.kr",
	"42header.username": "gyoon",
	"editor.minimap.enabled": true,
	"explorer.confirmDragAndDrop": false,

	// C++
	"C_Cpp.default.cppStandard": "c++98",
	"editor.defaultFormatter": "ms-vscode.cpptools",
	"editor.formatOnSave": true,
	"clang-format.language.c.enable": false,
	"C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: Microsoft, AlwaysBreakTemplateDeclarations: Yes, SpacesInAngles: Leave, AccessModifierOffset: -4, AllowShortFunctionsOnASingleLine: All, ColumnLimit : 80}", //AlignAfterOpenBracket: BlockIndent , AllowShortFunctionsOnASingleLine: All,
	"workbench.colorTheme": "GitHub Dark",

	"editor.tokenColorCustomizations": {
		"comments": "#ffffff54",
		"functions": "#c5ff83",
		"keywords": "#7bff00",
		"numbers": "#a4c8ff",
		"strings": "#c8f5ff",
		"types": "#ffffff",
		"variables": "#ffffff",
		"textMateRules": [
			{
				"scope" : "entity",
				"settings": {
					"foreground": "#ffffff",
				}
			},
			{
				"scope" : "keyword.control",
				"settings": {
					"foreground": "#fffa00",
				}
			},
			{
				"scope" : "meta",
				"settings": {
					"foreground": "#ffffffb6",
				}
			},
			{
				"scope" : "constant.language",
				"settings": {
					"foreground": "#ffbb00",
				}
			},
			{
				"scope" : "keyword.operator",
				"settings": {
					"foreground": "#f7f146",
				}
			},
			{
				"scope" : "entity.name.type",
				"settings": {
					"foreground": "#49da96",
				}
			},
			{
				"scope" : "storage.modifier",
				"settings": {
					"foreground": "#77fb03",
				}
			},
		]
	},

